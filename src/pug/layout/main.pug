include ../utils/mixins
block variables

doctype html
html(lang='en')
	head
		meta(charset='UTF-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge')
		meta(name='description', content=description)
		meta(name='viewport', content='width=device-width, initial-scale=1')
		title #{title}
		block head

	body(class=pageClass)
		block content
		block script
    
